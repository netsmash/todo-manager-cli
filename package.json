{
  "name": "todo-manager-cli",
  "version": "0.1.1",
  "description": "A simple CLI to manage tasks",
  "scripts": {
    "build": "npm run clean && tsc && cp package.json README.md dist/",
    "clean": "rm -rf ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/ --ext .ts,.tsx",
    "start": "ts-node src/index.ts",
    "test": "NODE_ENV=test && jest"
  },
  "bin": {
    "todo": "./index.js"
  },
  "author": "NetSmash",
  "license": "MIT",
  "keywords": [
    "todo",
    "todo-cli",
    "todo-app",
    "command-line",
    "manager",
    "todo-manager",
    "task-manager",
    "todolist",
    "cli",
    "yml",
    "yaml"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.12",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "^8.3.0",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "tmp-promise": "^3.0.3",
    "todo-manager": "^0.1.1",
    "yaml": "^2.0.0-10"
  }
}
